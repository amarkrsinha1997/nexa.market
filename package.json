{
  "name": "nexa.market",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "npm run docker:down && npm run docker:up",
    "db:generate": "prisma generate",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "setup": "npm install && npm run docker:up && npm run db:generate && npm run db:migrate:dev",
    "setup:prod": "npm ci && npm run db:generate && npm run db:migrate:deploy && npm run build",
    "dev:db": "npm run docker:up && next dev",
    "prod:start": "npm run build && next start"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@react-oauth/google": "^0.13.4",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "google-auth-library": "^10.5.0",
    "jose": "^6.1.3",
    "libnexa-ts": "^3.0.0",
    "lucide-react": "^0.563.0",
    "nexa-wallet-sdk": "^0.9.1-beta.2",
    "next": "16.1.6",
    "pg": "^8.18.0",
    "qrcode.react": "^4.2.0",
    "react": "19.2.3",
    "react-day-picker": "^9.13.0",
    "react-dom": "19.2.3",
    "tailwind-merge": "^3.4.0",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/web-push": "^3.6.4",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "prisma": "^7.3.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
